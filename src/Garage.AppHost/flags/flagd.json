{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "enable-chatbot": {
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "in": [
              {
                "var": "userId"
              },
              [
                "1"
              ]
            ]
          },
          "on"
        ]
      }
    },
    "enable-database-winners": {
      "state": "ENABLED",
      "variants": {
        "disabled": false,
        "enabled": true
      },
      "defaultVariant": "enabled"
    },
    "enable-preview-mode": {
      "state": "ENABLED",
      "variants": {
        "disabled": "",
        "enabled": "enable-tabs,enable-chatbot"
      },
      "defaultVariant": "enabled"
    },
    "enable-stats-header": {
      "state": "ENABLED",
      "variants": {
        "off": false,
        "on": true
      },
      "defaultVariant": "on"
    },
    "enable-tabs": {
      "state": "ENABLED",
      "variants": {
        "disabled": false,
        "enabled": true
      },
      "defaultVariant": "disabled",
      "targeting": {
        "if": [
          {
            "in": [
              {
                "var": "userId"
              },
              [
                "1"
              ]
            ]
          },
          "enabled"
        ]
      }
    },
    "prompt-file": {
      "state": "ENABLED",
      "variants": {
        "brief": "brief",
        "casual": "casual",
        "expert": "expert",
        "unreliable": "unreliable"
      },
      "defaultVariant": "expert"
    },
    "winners-count": {
      "state": "ENABLED",
      "variants": {
        "five": 5,
        "one-hundred": 100,
        "ten": 10,
        "zero": 0
      },
      "defaultVariant": "one-hundred"
    }
  }
}
